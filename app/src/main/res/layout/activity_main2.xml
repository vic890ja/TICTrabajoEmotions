<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F3E6C1"
    tools:context=".MainActivity2">

    <Button
        android:id="@+id/cerrar"
        android:layout_width="81dp"
        android:layout_height="48dp"
        android:layout_marginEnd="4dp"
        android:backgroundTint="#FFFFFF"
        android:onClick="cerrar"
        android:text="@string/boton3"
        app:layout_constraintBottom_toTopOf="@+id/tv2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/volver"
        android:layout_width="81dp"
        android:layout_height="48dp"
        android:layout_marginStart="4dp"
        android:backgroundTint="#FFFFFF"
        android:onClick="volver"
        android:text="@string/boton2"
        app:layout_constraintBottom_toTopOf="@+id/tv2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tv2"
        android:layout_width="375dp"
        android:layout_height="117dp"
        android:layout_marginTop="48dp"
        android:background="#F3E6C1"
        android:letterSpacing="0.2"
        android:text="@string/txt_2"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv3"
        android:layout_width="349dp"
        android:layout_height="200dp"
        android:layout_marginTop="28dp"
        android:background="#CFC3A0"
        android:text="@string/txt_3"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/c3" />

    <ImageButton
        android:id="@+id/c1"
        android:layout_width="80dp"
        android:layout_height="71dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_weight="1"
        android:background="#00FFFFFF"
        android:contentDescription="@string/c1"
        android:onClick="c1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv2"
        app:srcCompat="@drawable/_f601"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <ImageButton
        android:id="@+id/c3"
        android:layout_width="80dp"
        android:layout_height="76dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:layout_weight="1"
        android:background="#00FFFFFF"
        android:contentDescription="@string/c3"
        android:onClick="c3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/c2"
        app:srcCompat="@drawable/_f621"
        tools:ignore="ImageContrastCheck,ImageContrastCheck,DuplicateSpeakableTextCheck" />

    <ImageButton
        android:id="@+id/c2"
        android:layout_width="80dp"
        android:layout_height="85dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:layout_weight="1"
        android:background="#028BC34A"
        android:contentDescription="@string/c2"
        android:onClick="c2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/c1"
        app:srcCompat="@drawable/_f62d"
        tools:ignore="ImageContrastCheck,DuplicateSpeakableTextCheck" />

    <ImageButton
        android:id="@+id/c4"
        android:layout_width="82dp"
        android:layout_height="72dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="8dp"
        android:background="#00FFFFFF"
        android:contentDescription="@string/c4"
        android:onClick="c4"
        app:layout_constraintStart_toEndOf="@+id/c1"
        app:layout_constraintTop_toBottomOf="@+id/tv2"
        app:srcCompat="@drawable/frustradoni" />

    <ImageButton
        android:id="@+id/c5"
        android:layout_width="82dp"
        android:layout_height="79dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="28dp"
        android:background="#00FFFFFF"
        android:contentDescription="@string/c5"
        android:onClick="c5"
        app:layout_constraintStart_toEndOf="@+id/c2"
        app:layout_constraintTop_toBottomOf="@+id/c4"
        app:srcCompat="@drawable/enamorado" />

    <ImageButton
        android:id="@+id/c6"
        android:layout_width="80dp"
        android:layout_height="77dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="16dp"
        android:layout_weight="1"
        android:background="#028BC34A"
        android:contentDescription="@string/c6"
        android:onClick="c6"
        app:layout_constraintStart_toEndOf="@+id/c3"
        app:layout_constraintTop_toBottomOf="@+id/c5"
        app:srcCompat="@drawable/caritaconfundida"
        tools:ignore="ImageContrastCheck,DuplicateSpeakableTextCheck" />

    <ImageButton
        android:id="@+id/c7"
        android:layout_width="77dp"
        android:layout_height="73dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="20dp"
        android:layout_weight="1"
        android:background="#028BC34A"
        android:contentDescription="@string/c7"
        android:onClick="c7"
        app:layout_constraintStart_toEndOf="@+id/c6"
        app:layout_constraintTop_toBottomOf="@+id/c9"
        app:srcCompat="@drawable/asustao"
        tools:ignore="ImageContrastCheck,DuplicateSpeakableTextCheck" />

    <ImageButton
        android:id="@+id/c8"
        android:layout_width="81dp"
        android:layout_height="72dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="8dp"
        android:layout_weight="1"
        android:background="#028BC34A"
        android:contentDescription="@string/c8"
        android:onClick="c8"
        app:layout_constraintStart_toEndOf="@+id/c4"
        app:layout_constraintTop_toBottomOf="@+id/tv2"
        app:srcCompat="@drawable/facha"
        tools:ignore="ImageContrastCheck,DuplicateSpeakableTextCheck" />

    <ImageButton
        android:id="@+id/c9"
        android:layout_width="77dp"
        android:layout_height="75dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="24dp"
        android:layout_weight="1"
        android:background="#028BC34A"
        android:contentDescription="@string/c9"
        android:onClick="c9"
        app:layout_constraintStart_toEndOf="@+id/c5"
        app:layout_constraintTop_toBottomOf="@+id/c8"
        app:srcCompat="@drawable/payaso"
        tools:ignore="ImageContrastCheck,DuplicateSpeakableTextCheck" />

</android.support.constraint.ConstraintLayout>